      <span style="float:left;color:green;width:40px"><%=resource.id %></span>
      <span style="float:left" title="<%=resource.command_name %>">
        <%=resource.command_name[0..80] %>
      </span>
      <div style="display:inline;float:right;padding:0 4px;color:grey">
        <%=time_ago_in_words resource.updated_at %>前
      </div>
      <div style="display:inline;float:right">
        <%=actions(resource) %>
        <span onclick="try_alert('<%=resource.response %>',10000)">结果</span>
        <%=resource.human_state_name %>
      </div>
      <div style="clear:both" />

